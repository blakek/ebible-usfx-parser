{
  "name": "kjv-bible-json",
  "version": "0.0.1",
  "main": "dist/index.ts",
  "author": "Blake Knight <oss.ideas@gmail.com> (https://blakek.me/)",
  "license": "MIT",
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "ignoredByWatcher": [
      "!src/**"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "2m"
  },
  "devDependencies": {
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "ava": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-gsandf": "^1.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --fix",
    "setup": "bash setup.sh",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "ava"
  },
  "dependencies": {
    "@types/unist": "^2.0.3",
    "dot-prop": "^5.2.0",
    "sax": "^1.2.4",
    "unist-util-select": "^3.0.0"
  }
}
